{
    "name": "silvercommerce/payments-stripe-paymentintents",
    "description": "Package that adds Stripe payments (via omnipay) and includes Payment Intent integration",
      "type": "silverstripe-vendormodule",
      "homepage": "https://github.com/silvercommerce/payments-paypal",
      "keywords": [
          "silverstripe",
          "commerce",
          "ecommerce",
          "payments",
          "omnipay",
          "stripe"
      ],
      "license": "BSD-3-Clause",
      "authors": [
          {
              "name": "Morven Lewis-Everley",
              "email": "morven@ilateral.co.uk"
          },
          {
              "name": "Mark Anderson",
              "email": "mark@ilateral.co.uk"
          }
      ],
      "support": {
          "issues": "https://github.com/silvercommerce/payments-stripe-paymentintents/issues"
      },
      "require": {
          "silverstripe/framework": "^4.2",
          "silvercommerce/checkout": "^1.1",
          "silvercommerce/settings": "^1.0",
          "silvercommerce/orders-admin": "^1.3",
          "php-http/guzzle7-adapter": "^2.0",
          "omnipay/stripe": "^3.0"
      },
      "extra": {
          "expose": [
              "client"
          ]
      },
      "autoload": {
          "psr-4": {
              "SilverCommerce\\StripePaymentIntents\\Extensions\\": "src/"
          }
      }
  }
  
